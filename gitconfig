[alias]
  co = checkout
  dc = diff --cached
  lg = log --graph --pretty=format:'%C(red)%h%C(reset) -%C(yellow)%d%C(reset) %s %C(green)%cr %C(blue)<%an>%C(reset)' --abbrev-commit --date=relative
  st = status
  start = flow feature start
  finish = flow feature finish
  rstart = flow release start
  rfinish = flow release finish
[color]
  ui = auto
