[alias]
  co = checkout
  dc = diff --cached
  lg = log --graph --pretty=format:' %C(red)%h%C(reset) - %s\n %C(blue)<%an> %C(green)%ar%C(yellow)%d%C(reset)\n'
  lgd = log --graph --pretty=format:' %C(red)%h%C(reset) - %s\n %C(blue)<%an> %C(green)%ai%C(yellow)%d%C(reset)\n'
  st = status
  pr = remote prune
[color]
  ui = auto
