  name = Scott Nielsen
  email = scottnielsen5@gmail.com
[core]
  excludesfile = ~/.gitignore
[github]
  user = scizo
[push]
  default = upstream
[filter "trailingwhitespace"]
  clean = sed 's/[ \t]*$//'
  smudge = cat
[credential]
  helper = osxkeychain
[url "git@github.com:"]
  insteadOf = https://github.com/
#[commit]
#	gpgsign = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge --skip -- %f
	process = git-lfs filter-process --skip
	required = true
