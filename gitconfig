[alias]
  co = checkout
  dc = diff --cached
  lg = log --graph --pretty=format:' %C(red)%h%C(reset) - %s\n %C(blue)<%an> %C(green)%cr%C(yellow)%d%C(reset)\n'
  st = status
  start = flow feature start
  finish = flow feature finish
  rstart = flow release start
  rfinish = flow release finish
[color]
  ui = auto
